<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/black</item>
    </style>
    <style name="AppTheme.Popupoverlay" parent="AppTheme.Popupoverlay" tools:ignore="ResourceCycle">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/whitecolor</item>
        <item name="colorPrimaryDark">@color/whitecolor</item>
        <item name="colorAccent">@color/whitecolor</item>


    </style>
    <style name="AppTheme.AppbarOverlay" parent="AppTheme.AppbarOverlay" tools:ignore="ResourceCycle">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/whitecolor</item>
        <item name="colorAccent">@color/whitecolor</item>
        <item name="android:textColorHint">@color/offwhite</item>
        <item name="android:textColorPrimary">@color/whitecolor</item>
        <item name="android:textColorSecondary">@color/whitecolor</item>
    </style>


    <style name="MyTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="themeloginpreview" parent="AppTheme">
        <item name="android:windowContentTransitions">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowSharedElementEnterTransition">@transition/sharedelementspeed
        </item>
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryDark">#000000</item>

    </style>

    <style name="themegetphonenumber" parent="AppTheme">
